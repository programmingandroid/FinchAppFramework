<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mediaentities.readerframework"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name"
        android:uiOptions="splitActionBarWhenNarrow" android:theme="@android:style/Theme.Holo.Light" android:allowBackup="true" android:debuggable="true">
        <activity android:name="com.finchframework.uiframework.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.finchframework.uiframework.TabActivity"
            android:label="@string/data"></activity>
        

    </application>

</manifest>
